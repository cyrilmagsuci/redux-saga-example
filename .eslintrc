{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": [0],
    "indent": [2, "tab", {"SwitchCase": 1}],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-indent-props": [2, "tab"],
    "semi": [2, "always"]
  }
}
